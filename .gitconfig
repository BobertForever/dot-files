[user]
	name = Robert Lynch
	email = rmlynch@utexas.edu
[merge]
	tool = vimdiff
[core]
	editor = vim
[alias]
	st = status
	ci = commit
	co = checkout
	stashco = checkout stash@{0} -- 
	vimdiff = difftool -y -t vimdiff
	ls = log --stat --abbrev-commit
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
[color]
	branch = auto
	interactive = auto
	status = auto
	diff = auto
[diff]
	tool = vimdiff
